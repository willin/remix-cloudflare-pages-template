{
  "private": true,
  "name": "remix-template",
  "description": "",
  "author": "Willin Wang <willin@willin.org> (https://willin.wang/)",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:css && NODE_ENV=production remix build",
    "dev": "pm2-dev ./pm2.config.js  --env development",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "postinstall": "remix setup cloudflare-pages",
    "lint": "eslint --fix --ext .ts,.tsx --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@remix-run/cloudflare-pages": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@remix-run/dev": "^1.1.3",
    "@remix-run/eslint-config": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.11",
    "@willin/eslint-config": "^0.1.1",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.21",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "pm2": "^5.1.2",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5",
    "wrangler": "alpha"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
